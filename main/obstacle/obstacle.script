go.property("speed", 100)

function update(self, dt)
	if is_running == false then
		return
	end
	
	local p = go.get_position()
	p.x = p.x - self.speed * dt

	-- remove the object if out of bound
	if p.x < -100 then
		go.delete()
		return;
	end

	-- print("obstacle: pos=", p)
	go.set_position(p)
	
end
